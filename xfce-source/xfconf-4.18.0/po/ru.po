msgid "Xfce Configuration Daemon"
msgstr "Диспетчер Конфигурации Xfce"

#: ../xfconfd/main.c:181
msgid "Xfce configuration daemon"
msgstr "Диспетчер конфигурации Xfce"

#: ../xfconfd/main.c:183
msgid "Report bugs to http://bugs.xfce.org/\n"
msgstr "Отправляйте сообщения об ошибках на http://bugs.xfce.org/\n"

