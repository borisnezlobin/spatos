#, c-format
msgid "GType transformation failed \"%s\""
msgstr "Трансформация GType не удалась: «%s»"

#: ../xfconfd/xfconf-daemon.c:483
#, c-format
msgid "No backends could be started"
msgstr "Не удалось запустить бэкенд"

