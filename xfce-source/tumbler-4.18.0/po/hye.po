#: ../tumblerd/tumbler-specialized-thumbnailer.c:438
msgid "Failed to call the specialized thumbnailer: timeout"
msgstr "Չյաջողուեց կանչել յատկացուած փոքրապատկերը․ սպառում"

#: ../tumblerd/tumbler-specialized-thumbnailer.c:447
#, c-format
msgid "Failed to call the specialized thumbnailer: %s"
msgstr "Չյաջողուեց կանչել յատկացուած փոքրապատկերը․ %s"

#: ../tumbler/tumbler-enum-types.c:49
msgid "Invalid format"
msgstr "Անվաւեր ձեւաչափ"

#: ../tumbler/tumbler-enum-types.c:50
msgid "PNG"
msgstr "PNG"

#: ../tumbler/tumbler-enum-types.c:71
msgid "RGB Colorspace"
msgstr "RGB գունատարածք"

#: ../tumbler/tumbler-error.h:41
msgid "Thumbnail could not be inferred from file contents"
msgstr "Փոքրապատկերը չի կարող ստեղծուել նիշի բովանդակութեան միջից"

#: ../tumbler/tumbler-error.h:42
msgid "Only local files are supported"
msgstr "Սատարուում են միայն տեղային նիշերը"

#: ../tumbler/tumbler-error.h:43
#, c-format
msgid "Corrupt thumbnail PNG: '%s'"
msgstr ""

#: ../tumbler/tumbler-error.h:44
#, c-format
msgid "Could not save thumbnail to \"%s\""
msgstr "Չյաջողուեց պահել փոքրապատկերը \"%s\"֊ում"

#: ../tumbler/tumbler-error.h:45
#, c-format
msgid "The file \"%s\" is a thumbnail itself"
msgstr "\"%s\" նիշը համարուում է փոքրապատկեր"

#: ../tumbler/tumbler-error.h:46
#, c-format
msgid "No thumbnailer available for \"%s\""
msgstr "\"%s\"֊ի համար հասանելի փոքրապատկերներ չկան"

#: ../tumbler/tumbler-error.h:47
msgid "The thumbnailer service is shutting down"
msgstr "Փոքրապատկերների ծառայութիւնը անջատուում է"

#: ../tumbler/tumbler-error.h:48
msgid "Unsupported thumbnail flavor requested"
msgstr "Յայտ է ներկայացուել չսատարուող տարրի համար"

#: ../plugins/cover-thumbnailer/cover-thumbnailer.c:394
#, c-format
msgid "Failed to load the poster image \"%s\""
msgstr "Չյաջողուեց բեռնել \"%s\" յայտարարութեան պատկերը"

#: ../plugins/cover-thumbnailer/cover-thumbnailer.c:430
#, c-format
msgid "Failed to load the metadata from \"%s\""
msgstr "Չյաջողուեց բեռնել տուեալները \"%s\"֊ից"

#: ../plugins/cover-thumbnailer/cover-thumbnailer.c:626
msgid ""
"Invalid API key, you must be granted a valid key. The Movie DB backend will "
"be disabled."
msgstr "Անվաւեր API բանալի, պէտք է տրամադրէք վաւեր բանալի։ Տեսանիւթի DB backend֊ը կանջատուի։"

#: ../plugins/cover-thumbnailer/cover-thumbnailer.c:634
msgid "No poster key found in metadata"
msgstr "Յայտարարութեան ոչ մի բանալի մետատուեալներում չկա"

#: ../plugins/cover-thumbnailer/cover-thumbnailer.c:689
msgid "Movie title is too short"
msgstr "Տեսանիւթի անունը չափազանց կարճ է"

#. there was an error in the freetype initialization, abort
#: ../plugins/font-thumbnailer/font-thumbnailer.c:463
#, c-format
msgid "Could not initialize freetype: %s"
msgstr "Չյաջողուեց սկզբնաւորել freetype֊ը․ %s"

#. there was an error, emit error signal
#: ../plugins/font-thumbnailer/font-thumbnailer.c:475
#, c-format
msgid "Could not load file contents: %s"
msgstr "Չյաջողուեց բեռնել նիշի բովանդակութիւնը․ %s"

#. the font file could not be loaded, emit an error signal
#: ../plugins/font-thumbnailer/font-thumbnailer.c:494
#, c-format
msgid "Could not open font file: %s"
msgstr "Չյաջողուեց բացել տառատեսակի նիշը․ %s"

#. emit an error signal
#: ../plugins/font-thumbnailer/font-thumbnailer.c:518
#, c-format
msgid "Could not set the character map: %s"
msgstr "Չյաջողուեց տեղադրել գրանշանի քարտեզը․ %s"

#. emit an error signal
#: ../plugins/font-thumbnailer/font-thumbnailer.c:545
#, c-format
msgid "Could not render glyphs: %s"
msgstr "Չյաջողուեց ցուցադրել գլիֆները․ %s"

#: ../plugins/poppler-thumbnailer/poppler-thumbnailer.c:337
msgid "The document is empty"
msgstr "Փաստաթուղթը դատարկ է"

#: ../plugins/poppler-thumbnailer/poppler-thumbnailer.c:349
msgid "First page of the document could not be read"
msgstr "Փաստաթղթի առաջին էջը չի կարող ընթերցուել"
